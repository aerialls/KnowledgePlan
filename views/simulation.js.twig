{# This file represents the simulation #}

var simulation = new Object();

// Step
simulation.step = {{ simulation.step }};

// The time for the first entry
simulation.minTime = {{ simulation.minTime }};

// The time for the last entry
simulation.maxTime = {{ simulation.maxTime }};

// Plot options
simulation.plotOptions = {{ simulation.plotOptions|json_encode|raw }}

